    	         } ]
  	      } ,
                 {
    	         "name" : "error_count",
    	         "values" : [ {
      	            "timestamp" : 1335241800000,
      	            "value" : "0.0"
    	           } ]
  	    } 
              ],
  	
               "name" : "api1"
	}, 
            {
  	  "metrics" : [ 
               {
    	      "name" : "message_count",
    	      "values" : [ {
      	             "timestamp" : 1335241800000,
      	             "value" : "1.0"
    	          } ]
  	     },
                {
    	        "name" : "error_count",
    	        "values" : [ {
      	              "timestamp" : 1335241800000,
      	              "value" : "0.0"
    	        } ]
  	    } 
             ],
  	
             "name" : "api2"
	} 
          ],
        
         "name" : "test"
      } 
    ],
   "metaData" : {
    }
}</pre>

