	&quot;consumerSecret&quot; : &quot;HRlGbgdHLEwvzBhQ&quot;,
	&quot;status&quot; : &quot;approved&quot;
  }, {
	&quot;apiProducts&quot; : [ {
  	&quot;apiproduct&quot; : &quot;TestApiProduct&quot;,
